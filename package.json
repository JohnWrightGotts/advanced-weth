{
  "name": "advanced-weth",
  "version": "1.0.0",
  "description": "Wrapped WETH contract that adds advanced functionality to WETH",
  "author": "Moody Salem",
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.0.35",
    "bn.js": "^5.1.1",
    "canonical-weth": "^1.4.0",
    "truffle": "^5.1.26",
    "truffle-plugin-verify": "^0.3.10"
  },
  "scripts": {
    "compile": "truffle compile",
    "test": "truffle test",
    "deploy:testnets": "truffle migrate --network kovan && truffle migrate --network ropsten && truffle migrate --network rinkeby",
    "deploy:mainnet": "truffle migrate --network mainnet",
    "verify:testnets": "truffle run verify AdvancedWETH --network kovan && truffle run verify AdvancedWETH --network ropsten && truffle run verify AdvancedWETH --network rinkeby",
    "verify:mainnet": "truffle run verify AdvancedWETH --network mainnet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moodysalem/advanced-weth.git"
  },
  "files": [
    "build",
    "!build/contracts/WETH9.json",
    "!build/contracts/Migrations.json",
    "!build/contracts/TargetContract.json",
    "contracts/interfaces"
  ],
  "keywords": [
    "smart",
    "contract",
    "advanced",
    "ether",
    "wrap",
    "wrapped",
    "WETH",
    "advanced",
    "utility"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/moodysalem/advanced-weth/issues"
  },
  "homepage": "https://github.com/moodysalem/advanced-weth#readme"
}
